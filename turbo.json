{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
		"lint:style": {
			"outputs": []
		},
    "dev": {
      "cache": false
    },
		"docs#dev": {
			"env": ["APP_DOCS_PORT"]
		},
		"homes#dev": {
			"env": ["APP_HOME_PORT"]
		},
		"about#dev": {
			"env": ["APP_ABOUT_PORT"]
		}
  }
}
