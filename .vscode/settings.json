{
	"editor.defaultFormatter": "dbaeumer.vscode-eslint",
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"vue",
		"astro"
	],
	"stylelint.validate": [
		"css",
		"postcss",
		"vue",
		"astro"
	],
	"[astro]": {
		"editor.defaultFormatter": "astro-build.astro-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "stylelint.vscode-stylelint"
	}
}
