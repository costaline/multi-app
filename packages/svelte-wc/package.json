    "@storybook/svelte-vite": "7.0.0-beta.41",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "eslint-config-custom": "*",
    "storybook": "7.0.0-beta.41",
    "svelte": "^3.54.0",
